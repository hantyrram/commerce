{
 "$schema":"http://json-schema.org/draft-07/schema",
 "type":"object",
 "properties": {
  "_id": {
   "type": "string"
  },
  "name":{
   "type":"string",
   "minLength":1,
   "maxLength":45,
   "pattern": "^[^\\s]+$"
  },
  "label":{
   "type":"string",
   "minLength":6,
   "maxLength":60
  },
  "description":{
   "minLength":5,
   "maxLength":50
  }
 },
 "required": [
  "name","label"
 ]
}